<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- Period to start to collect the sensorML documents in seconds -->
	<entry key="capabilities_task_period">86400</entry>
	<!-- Period to start to collect the new observations in seconds -->
	<entry key="observations_task_period">300</entry>
	<!-- Minimum update time for new observations in seconds -->
	<entry key="update_observation_period">5</entry>
	<!-- Number of Maximum loaded procedures -->
	<entry key="maximum_number_procedures"></entry>
	<!-- Constraints for the procedure names as comma separated value -->
	<entry key="procedure_name_constraints"></entry>
    <!-- Prohibit procedure names as comma separated value -->
    <entry key="prohibit_procedure_names">general</entry>
    <!-- NoData wildcards as comma separated values -->
	<entry key="nodatas">-999999999</entry>
	<!-- Supported sos version -->
	<entry key="sos_version">1.0.0</entry>
    <!-- Start timestamp in seconds before actual time -->
    <entry key="start_timestamp">900</entry>
    <!-- Sleep time between two observations send to SES in milliseconds -->
    <entry key="sleep_time_observation">1000</entry>

	<!-- URL to the SES and some other configurations for the SES -->	
	<!-- <entry key="ses_url">http://localhost:8080/ses-main-3.0/services</entry> -->
	<!-- 
		kiribati
	 -->
	<entry key="ses_url">http://localhost:6080/ses/services</entry>
	<!-- 
		sensorweb.demo
	 -->
	<!-- <entry key="ses_url">http://sensorweb.demo.52north.org/SES-2010/services</entry> -->
	<entry key="ses_basic_port_type_path">/Broker</entry>
	<entry key="ses_version">0.0.0</entry>
	<entry key="ses_default_topic_dialect">http://docs.oasis-open.org/wsn/t-1/TopicExpression/Simple</entry>
	<entry key="ses_default_topic">ses:Measurements</entry>
	<entry key="ses_register_publisher_lifetime">2999-12-31T23:59:59+00:00</entry>
	<entry key="ses_register_publisher_endpoint">http://localhost:8080/</entry>
	
</properties>